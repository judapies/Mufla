.................... /*******************************************************************************  
.................... //Read SPI data  
....................  
.................... *******************************************************************************/   
.................... void readMAX()  
.................... {  
....................       output_low(SS);  
....................       delay_us(10);  
....................       spival[3]=spi_read(0);  
....................       delay_us(10);  
....................       spival[2]=spi_read(0);  
....................       delay_us(10);  
....................       spival[1]=spi_read(0);  
....................       delay_us(10);  
....................       spival[0]=spi_read(0);  
....................       delay_us(10);  
....................       output_high(SS);  
.................... }  
